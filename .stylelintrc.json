{
  "extends": "./node_modules/aurelia-bem/.stylelintrc.json",
  "rules": {
    "selector-max-specificity": [
      "0,1,0",
      {
        "ignoreSelectors": [
          ":visited",
          ":active",
          ":hover",
          ":link",
          ":focus",
          "::-moz-focus-inner"
        ]
      }
    ]
  }
}
